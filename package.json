    "@testing-library/react": "^14.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.34.0",
    "get-tsconfig": "^4.4.0",
    "react": "^18.2.0",
    "tiny-glob": "^0.2.9",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
