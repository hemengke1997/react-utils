    "@minko-fe/tsconfig": "latest",
    "@testing-library/react": "^14.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.17.15",
    "@types/prompts": "^2.4.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/validate-npm-package-name": "^4.0.0",
    "chokidar": "^3.5.3",
    "conventional-changelog-cli": "^2.2.2",
    "esbuild": "^0.17.19",
