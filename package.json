{
  "name": "@minko-fe/util-monorepo",
  "type": "module",
  "version": "0.0.58",
  "private": true,
  "packageManager": "pnpm@7.30.0",
  "description": "util monorepo",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "util"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel --filter=./packages/* dev",
    "build": "pnpm -r --filter ./packages/* build",
    "create": "tsx scripts/createLib",
    "test": "pnpm -r --filter=./packages/* run test --run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ignore-pattern \"/playground/\" .",
    "clean": "rimraf node_modules & pnpm -r --parallel --filter=./packages/* clean",
    "taze": "taze -I -r -w",
    "bump": "pnpm changelog && bumpp CHANGELOG.md packages/*/package.json package.json CHANGELOG.md --commit --push --tag -all"
  },
  "devDependencies": {
    "@minko-fe/eslint-config": "^1.2.29",
    "@minko-fe/lodash-pro": "latest",
    "@minko-fe/tsconfig": "^1.2.29",
    "@testing-library/react": "^14.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.3",
    "@types/prompts": "^2.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/validate-npm-package-name": "^4.0.0",
    "bumpp": "^9.0.0",
    "chokidar": "^3.5.3",
    "conventional-changelog-cli": "^2.2.2",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "fs-extra": "^11.1.0",
    "get-tsconfig": "^4.4.0",
    "joycon": "^3.1.1",
    "jsonc-simple-parser": "^3.0.0",
    "picocolors": "^1.0.0",
    "pnpm": "^7.30.0",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.0",
    "strip-dirs": "^3.0.0",
    "taze": "^0.9.0",
    "tiny-glob": "^0.2.9",
    "tsup": "^6.7.0",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5",
    "validate-npm-package-name": "^5.0.0",
    "vitest": "^0.29.3"
  }
}
