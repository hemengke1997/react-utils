  "scripts": {
    "dev": "pnpm -r --filter \"./packages/*\" dev",
    "build": "pnpm -r --filter \"./packages/*\" build",
    "create": "tsx scripts/createLib",
    "test": "pnpm -r --filter \"./packages/*\" run test --run",
    "prepare": "npx only-allow pnpm",
    "lint": "eslint --ignore-pattern \"/playground/\" . --fix",
    "clean": "rimraf node_modules & pnpm -r --parallel --filter \"./packages/*\"clean",
    "taze": "taze -I -r -w",
    "up": "pnpm update --i --L",
    "bump": "changeset",
    "version": "changeset version && changeset tag"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
