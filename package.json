    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.1",
    "strip-dirs": "^3.0.0",
    "taze": "^0.9.2",
    "tiny-glob": "^0.2.9",
    "tsup": "^6.7.0",
    "tsx": "^3.12.9",
    "typescript": "^5.2.2",
    "validate-npm-package-name": "^5.0.0",
    "vitest": "^0.29.8"
