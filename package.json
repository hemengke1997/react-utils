{
  "name": "@minko-fe/util-monorepo",
  "type": "module",
  "version": "0.0.12",
  "private": true,
  "packageManager": "pnpm@7.27.1",
  "description": "util monorepo",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "util"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel --filter=./packages/* dev && pnpm run watch",
    "build": "pnpm -r --parallel build --filter=\"{packages/*}\"",
    "watch": "tsx scripts/watch",
    "test": "pnpm -r --parallel --filter=./packages/* test",
    "changelog": "pnpm -r --parallel --filter=./packages/* changelog",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "upgrade:deps": "pnpm update --i --L",
    "bump": "pnpm changelog && bumpp packages/*/CHANGELOG.md packages/*/package.json package.json CHANGELOG.md --commit --push --tag -all"
  },
  "devDependencies": {
    "@minko-fe/eslint-config": "^1.2.20",
    "@minko-fe/tsconfig": "^1.2.20",
    "@testing-library/react": "^14.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "bumpp": "^8.2.1",
    "chokidar": "^3.5.3",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.34.0",
    "fs-extra": "^11.1.0",
    "get-tsconfig": "^4.4.0",
    "picocolors": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.2",
    "tiny-glob": "^0.2.9",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
