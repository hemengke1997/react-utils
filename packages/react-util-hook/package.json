{
  "name": "@minko-fe/react-util-hook",
  "type": "module",
  "version": "0.0.24",
  "description": "react util hook",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "react util"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx scripts/dev",
    "build": "tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "upgrade:deps": "pnpm update --i --L",
    "bump": "pnpm changelog && bumpp package.json CHANGELOG.md --commit --push --tag -all"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:^*",
    "ahooks": "^3.7.5",
    "query-string": "^8.1.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@types/query-string": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2"
  }
}
