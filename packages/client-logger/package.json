{
  "name": "@minko-fe/client-logger",
  "type": "module",
  "version": "0.0.67",
  "description": "client-logger",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "client-logger"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w",
    "bump": "bumpp package.json -c --all -p -t -x \"pnpm run changelog\""
  },
  "dependencies": {
    "@minko-fe/style-object-to-string": "^0.0.2",
    "dayjs": "^1.11.7"
  }
}
