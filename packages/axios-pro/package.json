{
  "name": "@minko-fe/axios-pro",
  "type": "module",
  "version": "0.0.72",
  "description": "axios-pro",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "axios-pro"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w",
    "bump": "bumpp package.json -c --all -p -t -x \"pnpm run changelog\""
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:*",
    "axios": "^1.3.5",
    "query-string": "7.1.3"
  },
  "devDependencies": {
    "@types/sinon": "^10.0.13",
    "nock": "^13.3.0",
    "sinon": "^15.0.3"
  }
}
