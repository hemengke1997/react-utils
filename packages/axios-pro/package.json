{
  "name": "@minko-fe/axios-pro",
  "type": "module",
  "version": "0.0.32",
  "description": "axios pro",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "util"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx scripts/dev",
    "build": "tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "upgrade:deps": "pnpm update --i --L",
    "bump": "pnpm changelog && bumpp package.json CHANGELOG.md --commit --push --tag -all"
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:*",
    "axios": "^1.3.3",
    "query-string": "^8.1.0"
  }
}
