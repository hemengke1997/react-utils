{
  "name": "@minko-fe/postcss-config",
  "type": "module",
  "version": "0.0.58",
  "description": "postcss-config",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "postcss-config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./tailwindcss/plugin": {
      "types": "./dist/tailwindcss/plugin/index.d.ts",
      "require": "./dist/tailwindcss/plugin/index.cjs",
      "import": "./dist/tailwindcss/plugin/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "tailwindcss/plugin": [
        "./dist/tailwindcss/plugin/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w",
    "bump": "pnpm changelog && bumpp package.json CHANGELOG.md --commit --push --tag -all"
  },
  "peerDependencies": {
    "postcss": ">=8.0.0"
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:*",
    "@minko-fe/postcss-pxtorem": "^1.1.1",
    "@minko-fe/postcss-pxtoviewport": "^1.1.1",
    "browserslist": "^4.21.5",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^8.0.1",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@types/postcss-import": "^14.0.0",
    "postcss": "^8.4.21"
  }
}
