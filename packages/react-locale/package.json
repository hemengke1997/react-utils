{
  "name": "@minko-fe/react-locale",
  "type": "module",
  "version": "0.0.60",
  "description": "react-locale",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "react-locale"
  ],
  "exports": {
    ".": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./plugin": {
      "types": "./dist/node/plugin/index.d.ts",
      "require": "./dist/node/plugin/index.cjs",
      "import": "./dist/node/plugin/index.js"
    }
  },
  "main": "./dist/client/index.js",
  "module": "./dist/client/index.js",
  "types": "./dist/client/index.d.ts",
  "typesVersions": {
    "*": {
      "plugin": [
        "./dist/node/plugin/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "resource.d.ts",
    "i18n.d.ts"
  ],
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w",
    "bump": "pnpm changelog && bumpp package.json CHANGELOG.md --commit --push --tag -all"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vite": ">=4.0.0"
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:*",
    "@minko-fe/vite-config": "workspace:*",
    "depth": "^0.1.1",
    "fs-extra": "^11.1.0",
    "i18next": "^22.4.12",
    "i18next-browser-languagedetector": "^7.0.1",
    "jsonc-simple-parser": "^3.0.0",
    "parse-glob": "^3.0.4",
    "query-string": "^8.1.0",
    "react-i18next": "^12.2.0",
    "strip-dirs": "^3.0.0",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@types/parse-glob": "^3.0.29",
    "react": "^18.2.0",
    "vite": "^4.2.0"
  }
}
