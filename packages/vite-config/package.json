{
  "name": "@minko-fe/vite-config",
  "version": "1.0.2",
  "description": "vite-config",
  "type": "module",
  "keywords": [
    "vite-config"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "*.d.ts",
    "CHANGELOG.md",
    "README.md",
    "client",
    "dist"
  ],
  "main": "./dist/node/index.cjs",
  "module": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./*": "./*",
    "./client": {
      "types": "./client.d.ts",
      "require": "./dist/client/index.cjs",
      "import": "./dist/client/index.js"
    },
    "./client/manifest": {
      "types": "./dist/client/manifest.d.ts",
      "require": "./dist/client/manifest.cjs",
      "import": "./dist/client/manifest.js"
    },
    "./plugin-client": {
      "types": "./plugin-client.d.ts"
    }
  },
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "test": "vitest --run",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w"
  },
  "peerDependencies": {
    "vite": ">=5.0.0",
    "vite-plugin-public-typescript": ">=3.1.1"
  },
  "peerDependenciesMeta": {
    "@vitejs/plugin-react": {
      "optional": true
    },
    "vite-plugin-public-typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:*",
    "@vitejs/plugin-legacy": "^5.4.1",
    "debug": "^4.3.5",
    "fast-glob": "^3.3.2",
    "picologger": "^0.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "std-env": "^3.7.0",
    "terser": "^5.31.1",
    "vconsole": "^3.15.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-public-typescript": "^3.2.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "rollup": "^4.18.0",
    "vite": "^5.3.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss-*",
        "@csstools/*"
      ]
    }
  }
}
