{
  "name": "@minko-fe/vite-config",
  "type": "module",
  "version": "0.0.71-beta.2",
  "description": "vite-config",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "vite-config"
  ],
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    }
  },
  "main": "./dist/node/index.cjs",
  "module": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "svgr.d.ts"
  ],
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w",
    "bump": "bumpp package.json -c --all -p -t -x \"pnpm run changelog\""
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@vitejs/plugin-react": {
      "optional": true
    }
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:*",
    "@vitejs/plugin-legacy": "^4.0.2",
    "debug": "^4.3.4",
    "get-tsconfig": "^4.5.0",
    "picologger": "^0.0.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "terser": "^5.16.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-public-typescript": "^1.3.2",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "rollup": "^3.20.2",
    "vite": "^4.2.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss-*",
        "@csstools/*"
      ]
    }
  }
}
