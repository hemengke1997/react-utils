{
  "name": "@minko-fe/vite-config",
  "type": "module",
  "version": "0.0.8",
  "description": "common vite config",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "vite config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx scripts/dev",
    "build": "tsup",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "upgrade:deps": "pnpm update --i --L",
    "bump": "pnpm changelog && bumpp package.json CHANGELOG.md --commit --push --tag -all"
  },
  "peerDependencies": {
    "@vitejs/plugin-react": ">=3.0.0",
    "vite": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@vitejs/plugin-react": {
      "optional": true
    }
  },
  "devDependencies": {
    "vite": "^4.1.3"
  }
}
