{
  "name": "@minko-fe/react-component",
  "type": "module",
  "version": "0.0.71-beta.9",
  "description": "react-component",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "keywords": [
    "react-component"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "require": "./dist/icons/index.js",
      "import": "./dist/esm/icons/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "icons": [
        "./dist/icons/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w",
    "bump": "bumpp package.json -c --all -p -t -x \"pnpm run changelog\""
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@minko-fe/lodash-pro": "workspace:*",
    "@minko-fe/react-hook": "workspace:*",
    "classnames": "^2.3.2",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@minko-fe/postcss-config": "workspace:*",
    "@types/react-transition-group": "^4.4.5",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.1"
  }
}
