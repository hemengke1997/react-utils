{
  "name": "@minko-fe/react-hook",
  "version": "0.0.83",
  "description": "react-hook",
  "type": "module",
  "keywords": [
    "react-hook"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/util.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "rimraf dist && tsx scripts/dev",
    "build": "rimraf dist && tsx scripts/build",
    "release": "pnpm run build && pnpm publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "lint": "eslint .",
    "clean": "rimraf node_modules",
    "up": "taze -I -w",
    "bump": "bumpp package.json -c --all -p -t -x \"pnpm run changelog\""
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "dependencies": {
    "@minko-fe/lodash-pro": "workspace:*",
    "ahooks": "^3.7.8",
    "query-string": "7.1.3",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  }
}
